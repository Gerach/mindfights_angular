{
  "name": "Mindfights",
  "version": "0.1.0",
  "description": "Mindfights electron application",
  "author": {
    "name": "Gerardas Martynovas",
    "email": "gerardas.martynovas@mif.stud.vu.lt",
    "url": "https://github.com/Gerach/"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "package-mac": "electron-packager . Mindfights --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds --asar",
    "package-linux": "electron-packager . Mindfights --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds --asar",
    "package-windows": "electron-packager . Mindfights --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds --asar",
    "create-mac-installer": "electron-installer-dmg --src release-builds/Mindfights-darwin-x64/",
    "create-linux-installer": "electron-installer-debian --src release-builds/Mindfights-linux-x64/ --arch amd64 --config debian.json",
    "create-windows-installer": "electron-installer-windows --src release-builds/Mindfights-win32-x64/ --arch amd64 --config windows.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "jquery": "^3.4.1",
    "nedb": "^1.8.0",
    "ngx-carousel-lib": "^0.1.2",
    "ngx-electron": "^2.1.1",
    "popper.js": "^1.14.7",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "^4.1.1",
    "electron-builder": "^20.43.0",
    "electron-installer-debian": "^2.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-installer-windows": "^2.0.0",
    "electron-packager": "^13.1.1",
    "electron-prebuilt": "^1.4.13",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nuget": "^2.0.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
